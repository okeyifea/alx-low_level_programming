#include <stdio.h>

int main()
 {
    char password [10], username [10], ch;
    int s,  i;
    printf("Enter username: ");
    gets(username);
    printf("Enter 8 digit password : ");
    for (i = 0; i < 8; i++){
        password[i] = getchar();
    }
    for(s = 0; s < 8; s++)
    {
        printf("%c",password [s]);
    }
    return 0;
}
